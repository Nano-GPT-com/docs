{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "NanoGPT API Documentation",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Get Started",
        "pages": [
          "introduction",
          "quickstart",
          "development"
        ]
      },
      {
        "group": "Essentials",
        "pages": [
          "essentials/markdown",
          "essentials/code",
          "essentials/images",
          "essentials/settings",
          "essentials/navigation",
          "essentials/reusable-snippets"
        ]
      },
      {
        "group": "API Reference",
        "pages": [
          "api-reference/text-generation",
          "api-reference/image-generation",
          "api-reference/video-generation"
        ]
      },
      {
        "group": "Endpoint Examples",
        "pages": [
          "api-reference/endpoint/get",
          "api-reference/endpoint/create",
          "api-reference/endpoint/delete",
          "api-reference/endpoint/webhook"
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://nano-gpt.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/KaQt8gPG6V",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://nano-gpt.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@nano-gpt.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.nano-gpt.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/nanogpt",
      "github": "https://github.com/nanogpt",
      "linkedin": "https://linkedin.com/company/nanogpt"
    }
  }
